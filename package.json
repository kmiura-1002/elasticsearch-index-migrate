{
    "name": "elasticsearch-index-migrate",
    "description": "This software is a command line tool for version control and mapping changes of Elasticsearch mappings.",
    "version": "1.0.0",
    "author": "kmiura @kmiura-1002",
    "private": false,
    "bin": {
        "elasticsearch-index-migrate": "./bin/run"
    },
    "bugs": "https://github.com/kmiura-1002/elasticsearch-index-migrate/issues",
    "dependencies": {
        "@oclif/core": "^1.9.0",
        "@oclif/plugin-help": "^5.1.12",
        "@oclif/plugin-version": "^1.0.4",
        "@opensearch-project/opensearch": "^1.0.2",
        "@types/checksum": "^0.1.33",
        "@types/js-yaml": "^4.0.5",
        "@types/lodash.merge": "^4.6.7",
        "checksum": "^1.0.0",
        "cli-ux": "^6.0.9",
        "date-fns": "^2.28.0",
        "es6": "npm:@elastic/elasticsearch@^6.8.8",
        "es7": "npm:@elastic/elasticsearch@^7.7.1",
        "inversify": "^6.0.1",
        "js-yaml": "^4.1.0",
        "jsondiffpatch": "^0.4.1",
        "lodash.merge": "^4.6.2",
        "prettier": "^2.6.2",
        "reflect-metadata": "^0.1.13",
        "semver": "^7.3.7",
        "ts-essentials": "^9.1.2",
        "ts-json-validator": "^0.7.1",
        "tslib": "^2.4.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.10",
        "@oclif/test": "^2.1.0",
        "@types/inversify": "^2.0.33",
        "@types/jest": "^28.1.0",
        "@types/node": "^17.0.38",
        "@types/power-assert": "^1.5.8",
        "@types/semver": "^7.3.9",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "chai": "^4.3.6",
        "eslint": "^8.16.0",
        "eslint-config-oclif-typescript": "^1.0.2",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "globby": "^11.0.0",
        "jest": "^28.1.0",
        "jest-mock": "^27.5.1",
        "lint-staged": "^10.5.4",
        "nyc": "^15.1.0",
        "power-assert": "^1.6.1",
        "ts-jest": "^28.0.3",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
    },
    "engines": {
        "node": ">=16.15.1",
        "npm": ">=6.9"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/kmiura-1002/elasticsearch-index-migrate",
    "keywords": [
        "oclif",
        "elasticsearch",
        "migration",
        "cli"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "additionalHelpFlags": [
            "-h"
        ],
        "additionalVersionFlags": [
            "-v"
        ],
        "commands": "./lib/app/commands",
        "bin": "elasticsearch-index-migrate",
        "plugins": [
            "@oclif/plugin-version",
            "@oclif/plugin-help"
        ],
        "topicSeparator": " ",
        "topics": {},
        "hooks": {}
    },
    "repository": "kmiura-1002/elasticsearch-index-migrate",
    "scripts": {
        "lint": "eslint -c .eslintrc 'src/**/*.ts'",
        "format": "prettier --write \"{src/**/*.ts,test/**/*.ts}\"",
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "yarn lint",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "build": "rm -f oclif.manifest.json && rm -rf lib && tsc -b && oclif-dev manifest",
        "test": "jest --colors --coverage",
        "version": "oclif-dev readme",
        "docker:build": "docker build -t kmiura1002/elasticsearch-index-migrate:`npx -c 'echo \"$npm_package_version\"'` ./docker",
        "readme": "oclif-dev readme"
    },
    "types": "lib/index.d.ts"
}
