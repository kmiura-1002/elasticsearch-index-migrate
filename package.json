{
    "name": "elasticsearch-index-migrate",
    "description": "This software is a command line tool for version control and mapping changes of Elasticsearch mappings.",
    "version": "0.7.3",
    "author": "kmiura @kmiura-1002",
    "private": false,
    "bin": {
        "elasticsearch-index-migrate": "./bin/run"
    },
    "bugs": "https://github.com/kmiura-1002/elasticsearch-index-migrate/issues",
    "dependencies": {
        "@oclif/command": "^1.8.13",
        "@oclif/config": "^1.15.1",
        "@oclif/plugin-help": "^3.2.17",
        "@opensearch-project/opensearch": "^1.0.2",
        "@types/js-yaml": "^4.0.5",
        "@types/lodash.merge": "^4.6.6",
        "checksum": "^1.0.0",
        "cli-ux": "^6.0.6",
        "date-fns": "^2.27.0",
        "es6": "npm:@elastic/elasticsearch@^6.8.8",
        "es7": "npm:@elastic/elasticsearch@^7.7.1",
        "inversify": "^6.0.1",
        "js-yaml": "^4.1.0",
        "jsondiffpatch": "^0.4.1",
        "lodash.merge": "^4.6.2",
        "prettier": "^2.5.1",
        "reflect-metadata": "^0.1.13",
        "semver": "^7.3.4",
        "ts-essentials": "^9.1.2",
        "ts-json-validator": "^0.7.1",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@oclif/test": "^1",
        "@types/inversify": "^2.0.33",
        "@types/jest": "^27.0.3",
        "@types/node": "^16.11.12",
        "@types/power-assert": "^1.5.8",
        "@types/semver": "^7.3.9",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "chai": "^4.3.6",
        "eslint": "^8.4.1",
        "eslint-config-oclif-typescript": "^1.0.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "globby": "^11.0.0",
        "husky": "^5.1.3",
        "jest": "^27.4.5",
        "jest-mock": "^27.4.2",
        "lint-staged": "^10.5.4",
        "nyc": "^15.1.0",
        "power-assert": "^1.6.1",
        "ts-jest": "^27.1.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.3",
        "yarn-audit-fix": "^7.0.4"
    },
    "engines": {
        "node": ">=10.15.0",
        "npm": ">=6.9"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/kmiura-1002/elasticsearch-index-migrate",
    "keywords": [
        "oclif",
        "elasticsearch",
        "migration",
        "cli"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/app/commands",
        "bin": "elasticsearch-index-migrate",
        "plugins": [
            "@oclif/plugin-help"
        ],
        "topicSeparator": ":",
        "topics": {
            "baseline:esindex": {
                "description": "plan:index"
            },
            "baseline:template": {
                "description": "plan:template"
            },
            "clean:esindex": {
                "description": "plan:index"
            },
            "clean:template": {
                "description": "plan:template"
            },
            "plan:esindex": {
                "description": "plan:index"
            },
            "plan:template": {
                "description": "plan:template"
            },
            "migrate:esindex": {
                "description": "plan:index"
            },
            "migrate:template": {
                "description": "plan:template"
            },
            "validate:esindex": {
                "description": "plan:index"
            },
            "validate:template": {
                "description": "plan:template"
            }
        },
        "hooks": {}
    },
    "repository": "kmiura-1002/elasticsearch-index-migrate",
    "scripts": {
        "lint": "eslint -c .eslintrc 'src/**/*.ts'",
        "format": "prettier --write \"{src/**/*.ts,test/**/*.ts}\"",
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "yarn lint",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "jest --colors --coverage",
        "version": "oclif-dev readme",
        "docker:build": "docker build -t kmiura1002/elasticsearch-index-migrate:`npx -c 'echo \"$npm_package_version\"'` ./docker",
        "readme": "oclif-dev readme",
        "prepare": "husky install"
    },
    "types": "lib/index.d.ts"
}
